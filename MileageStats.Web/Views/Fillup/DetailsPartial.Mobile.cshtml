@model MileageStats.Web.Models.FillupViewModel
<dl class="list">
	
		<dt>@Html.LabelFor(model => model.Date)</dt>
        <dd>@Model.Date</dd>

		<dt>@Html.LabelFor(model => model.TotalCost)</dt>
        <dd>@Model.TotalCost</dd>

		<dt>@Html.LabelFor(model => model.TotalUnits)</dt>
        <dd>@Model.TotalUnits</dd>

		<dt>@Html.LabelFor(model => model.PricePerUnit)</dt>
        <dd>@Model.PricePerUnit</dd>

		<dt>@Html.LabelFor(model => model.Vendor)</dt>
        <dd>@Model.Vendor</dd>

		<dt>@Html.LabelFor(model => model.Odometer)</dt>
        <dd>@Model.Odometer</dd>

		<dt>@Html.LabelFor(model => model.TransactionFee)</dt>
        <dd>@Model.TransactionFee</dd>

		<dt>@Html.LabelFor(model => model.Remarks)</dt>
        <dd>@(Model.Remarks ?? "None")</dd>
</dl>