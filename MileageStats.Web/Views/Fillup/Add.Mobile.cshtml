@model MileageStats.Web.Models.FillupEntryFormModel
@{
    ViewBag.Title = "Add Fill up";
    Layout = "~/Views/Shared/_Layout.Mobile.cshtml";
}

<nav>
    <ol class="title">
        <li><a href="@Url.Action("Index", "Dashboard")" class="dashboard icon only"><span>Dashboard</span></a></li>
        <li>@ViewBag.VehicleName</li>
    </ol>
    <ol class="breadcrumbs">
	    <li><a href="@Url.Action("List", "Fillup", new { vehicleId = Model.VehicleId })">Fillups</a></li>
	    <li>Add Fillup</li>
    </ol>
</nav>

<div class="view">
@using(Html.BeginForm()) { 
    if (Model != null) 
    {
        Html.RenderPartial("FillupForm.Mobile", Model);
    }
}
</div>