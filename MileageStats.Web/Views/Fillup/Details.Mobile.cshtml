@model MileageStats.Web.Models.FillupViewModel
@{
    ViewBag.Title = "Fill ups for " + ViewBag.VehicleName;
    Layout = "~/Views/Shared/_Layout.Mobile.cshtml";
}

<nav>
    <ol class="title">
        <li><a href="@Url.Action("Index", "Dashboard")" class="dashboard icon only"><span>Dashboard</span></a></li>
        <li>@ViewBag.VehicleName</li>
    </ol>
    <ol class="breadcrumbs">
	    <li><a href="@Url.Action("List", "Fillup", new { vehicleId = ViewBag.VehicleId })">Fillups</a></li>
	    <li>@Model.FillupEntryId</li>
    </ol>
</nav>

<div class="view">
    @{
		Html.RenderPartial("DetailsPartial.Mobile", Model);
    }
    <ol class="hbox button-group">
		<li><a href="@Url.Action("List", "Fillup", new { vehicleId = ViewBag.VehicleId })"><span>Back</span></a></li>
	</ol>
</div>
